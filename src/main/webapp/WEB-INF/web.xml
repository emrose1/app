<?xml version="1.0" encoding="UTF-8"?>
  <web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  
  <servlet>
    <servlet-name>SystemServiceServlet</servlet-name>
    <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
    <init-param>
      <param-name>services</param-name>
      <param-value>com.studio.bookings.service.EventService</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>SystemServiceServlet</servlet-name>
    <url-pattern>/_ah/spi/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TestHarness</servlet-name>
    <servlet-class>com.studio.bookings.TestHarnessServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ParentTesting</servlet-name>
    <servlet-class>com.studio.bookings.ParentTestingServlet</servlet-class>
  </servlet>
  <servlet>
    <description></description>
    <display-name>EventServiceTestingServlet</display-name>
    <servlet-name>EventServiceTestingServlet</servlet-name>
    <servlet-class>com.studio.bookings.EventServiceTestingServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>JodaTimeTesting</servlet-name>
    <servlet-class>com.studio.bookings.JodaTimeTestingServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>com.studio.bookings.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.studio.bookings.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>index</servlet-name>
    <jsp-file>app/index.html</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>events</servlet-name>
    <jsp-file>/events.jsp</jsp-file>
  </servlet>
  <servlet>
    <servlet-name>users</servlet-name>
    <jsp-file>/users.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>TestHarness</servlet-name>
    <url-pattern>/testharness</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>JodaTimeTesting</servlet-name>
    <url-pattern>/jodatimetesting</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ParentTesting</servlet-name>
    <url-pattern>/parenttesting</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>index</servlet-name>
    <url-pattern>/index.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>events</servlet-name>
    <url-pattern>/events.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>users</servlet-name>
    <url-pattern>/users.html</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EventServiceTestingServlet</servlet-name>
    <url-pattern>/eventservice</url-pattern>
  </servlet-mapping>
</web-app>